{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Database=your_database_name;Username=postgres;Password=your_password"
  },
  "JwtConfig": {
    "Issuer": "http://localhost:5000",
    "Audience": "http://localhost:3000/",
    "Key": "very very secret key for jwt key",
    "DurationInMinutes": 60
  },
  "CookieConfig": {
    "JwtKey": "jwt"
  },
  "AllowedHosts": "*"
}
